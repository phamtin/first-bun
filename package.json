{
  "name": "bun-trial",
  "version": "1.0.50",
  "scripts": {
    "lint": "bun tsc --noEmit && bunx biome lint --write ./src",
    "dev": "./start-dev.sh",
    "dev-m1": "bun tsc --noEmit && bun run --watch src/index.ts",
    "dcu": "docker compose -f ./docker-compose.dev.yaml up -d",
    "dcd": "docker compose -f ./docker-compose.dev.yaml down --remove-orphans"
  },
  "dependencies": {
    "@hono/valibot-validator": "^0.5.2",
    "bullmq": "^5.41.8",
    "dayjs": "^1.11.13",
    "hono": "^4.7.4",
    "ioredis": "^5.6.0",
    "mongodb": "^6.14.2",
    "valibot": "^1.0.0-rc.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "^1.2.5",
    "typescript": "^5.8.2"
  },
  "module": "src/index.js"
}